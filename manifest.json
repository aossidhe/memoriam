{
    "manifest_version": 2,
    "name":    "Memoriam",
    "version": "0.2",
    "description": "Now 30% more functional!",
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js":      ["content.js"]
    }],
    "browser_action": {
        "default_title": "Test extension"
    },
    "permissions": ["activeTab",
                    "tabs",
                    "*://*.youtube.com/*",
                    "webNavigation"]
}
